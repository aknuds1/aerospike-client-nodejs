{
  "name": "@arve.knudsen/aerospike",
  "version": "3.7.1",
  "description": "Aerospike Client Library",
  "keywords": [
    "aerospike",
    "database",
    "nosql"
  ],
  "homepage": "https://github.com/aknuds1/aerospike-client-nodejs",
  "license": "Apache-2.0",
  "main": "lib/aerospike",
  "engines": {
    "node": ">=4"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aknuds1/aerospike-client-nodejs"
  },
  "bugs": {
    "url": "https://github.com/aknuds1/aerospike-client-nodejs/issues"
  },
  "scripts": {
    "test": "mocha",
    "posttest": "standard",
    "coverage": "nyc npm test && nyc --reporter=lcov report",
    "codecov": "codecov",
    "prepush": "npm test",
    "apidocs": "jsdoc -c jsdoc.json",
    "cppcheck": "cppcheck --quiet --enable=warning,style -I src/include src/main/",
    "valgrind": "valgrind node ./node_modules/mocha/bin/_mocha -R dot -g '#slow' -i"
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "minimatch": "^3.0.4",
    "nan": "^2.11.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "choma": "^1.2.1",
    "codecov": "^3.1.0",
    "deep-eql": "^3.0.1",
    "dirty-chai": "^2.0.1",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mocha-clean": "^1.0.0",
    "nyc": "^11.2.1",
    "standard": "^11.0",
    "tmp": "^0.0.33",
    "yargs": "^1.3.3"
  },
  "standard": {
    "ignore": [
      "apidocs"
    ]
  },
  "files": [
    "aerospike-client-c.ini",
    "aerospike-client-c.sha256",
    "binding.gyp",
    "benchmarks/",
    "examples/",
    "lib/",
    "modules/lua-core/",
    "scripts/",
    "src/",
    "test/"
  ]
}
